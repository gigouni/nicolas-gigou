{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": "src/**/*.mjs",
        "exclude": [
            "node_modules",
            "config"
        ]
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "systemName"            : "Nicolas Gigou server documentation",
        "copyright"             :  "&copy; Nicolas Gigou 2018",
        "includeDate"           : true,
        "theme"                 : "Superhero",
        "linenums"              : true,
        "inverseNav"            : true,
        "outputSourceFiles"     : true,
        "dateFormat"            : "dddd, MMMM Do YYYY, h:mm:ss a"
    },
    "opts": {
        "destination": "docs",
        "private": true,
        "readme": "./README.md",
        "recurse": true,
        "template": "node_modules/ink-docstrap/template"
    }
  }